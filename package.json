{
  "name": "claim",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.15.0"
  },
  "scripts": {
    "dev": "concurrently \"tsx scripts/dev.ts\" \"tsx scripts/GameEditor.ts\"",
    "dev:clean": "concurrently \"tsx scripts/dev.ts\" \"tsx scripts/GameEditor.ts\"",
    "build": "tsc -b && vite build",
    "lint": "eslint . && npm run type-check",
    "lint:fix": "eslint . --fix",
    "lint:eslint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "type-check": "tsc -b --force && tsc --noEmit --project infra/cloudflare/tsconfig.json",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:integration": "vitest --run src/services/__tests__/integration",
    "test:load": "vitest --run src/services/__tests__/load",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:firebase": "node scripts/test-firebase.cjs",
    "debug:auth": "node scripts/debug-auth.cjs",
    "verify:matches": "tsx scripts/verify-matches.ts"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.32.1",
    "@huggingface/transformers": "^3.7.6",
    "@solana/spl-memo": "^0.2.5",
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/wallet-adapter-wallets": "^0.19.37",
    "@solana/web3.js": "^1.98.4",
    "@tanstack/react-query": "^5.90.6",
    "@types/better-sqlite3": "^7.6.13",
    "better-sqlite3": "^12.4.1",
    "bs58": "^6.0.0",
    "dotenv": "^17.2.3",
    "firebase": "^12.5.0",
    "kokoro-js": "^1.2.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.5",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.34.4",
    "three": "^0.181.0",
    "three-stdlib": "^2.36.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@eslint/js": "^9.36.0",
    "@modelcontextprotocol/sdk": "^1.21.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/three": "^0.181.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/ui": "^4.0.6",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^27.1.0",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "npm:rolldown-vite@7.1.14",
    "vitest": "^4.0.6",
    "wrangler": "^4.47.0",
    "ws": "^8.18.3"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
  }
}
